<?xml version="1.0"?>
<launch>
	
	<param name="/use_sim_time" value="false"/>
	<arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>
	<arg name="robot_env" default="$(optenv ROBOT_ENV !!NO_ROBOT_ENV_SET!!)"/>
	<arg name="pkg_env_config" default="$(find cob_default_env_config)"/>

	<!-- start cob_multicomp_twist_controller -->

	<!-- arm left -->
	<node ns="/arm_left" pkg="cob_multicomp_twist_controller" 	type="cob_multicomp_twist_controller_node" name="cob_multicomp_twist_controller" respawn="false" output="screen"/>

	<!-- arm right -->
	<node ns="/arm_right" pkg="cob_multicomp_twist_controller" type="cob_multicomp_twist_controller_node" name="cob_multicomp_twist_controller" respawn="false" output="screen"/>

	<!-- sensorring -->
	<node ns="/sensorring" pkg="cob_multicomp_twist_controller" type="cob_multicomp_twist_controller_node" name="cob_multicomp_twist_controller" respawn="false" output="screen"/>


	<!-- head
	<node ns="/head" pkg="cob_multicomp_twist_controller" type="cob_multicomp_twist_controller_node" name="cob_multicomp_twist_controller" respawn="false" output="screen"/>
	-->

	<!-- torso
	<node ns="/torso" pkg="cob_multicomp_twist_controller" type="cob_multicomp_twist_controller_node" name="cob_multicomp_twist_controller" respawn="false" output="screen"/>
	-->
	
</launch>

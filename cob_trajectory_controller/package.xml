<package format="2">
  <name>cob_trajectory_controller</name>
  <version>0.7.8</version>
  <description>This package provides a trajectory controller which controlls velocities for a chain of joints. This controller can be used e.g. with [[schunk_powercube_chain]].</description>

  <license>Apache 2.0</license>

  <url type="website">http://ros.org/wiki/cob_trajectory_controller</url>
  <!-- <url type="bugtracker"></url> -->

  <maintainer email="aub@ipa.fhg.de">Alexander Bubeck</maintainer>

  <author>Alexander Bubeck</author>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>actionlib</depend>
  <depend>cob_srvs</depend>
  <depend>control_msgs</depend>
  <depend>dynamic_reconfigure</depend>
  <depend>roscpp</depend>
  <depend>sensor_msgs</depend>
  <depend>std_msgs</depend>
  <depend>std_srvs</depend>
  <depend>trajectory_msgs</depend>

</package>

<?xml version="1.0"?>
<package format="2">
  <name>cob_tricycle_controller</name>
  <version>0.8.10</version>
  <description>The cob_omni_drive_controller package provides a ros_controller plugin for the Care-O-bot tricycle base platform.</description>

  <author email="felix.messmer@mojin-robotics.de">Felix Messmer</author>
  <maintainer email="felix.messmer@mojin-robotics.de">Felix Messmer</maintainer>

  <license>Apache 2.0</license>


  <buildtool_depend>catkin</buildtool_depend>

  <depend>angles</depend>
  <depend>boost</depend>
  <depend>cob_base_controller_utils</depend>
  <depend>controller_interface</depend>
  <depend>geometry_msgs</depend>
  <depend>hardware_interface</depend>
  <depend>nav_msgs</depend>
  <depend>pluginlib</depend>
  <depend>realtime_tools</depend>
  <depend>roscpp</depend>
  <depend>std_srvs</depend>
  <depend>tf</depend>

  <export>
    <controller_interface plugin="${prefix}/controller_plugins.xml"/>
  </export>
</package>
